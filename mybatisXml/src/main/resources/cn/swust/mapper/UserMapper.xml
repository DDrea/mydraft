<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.swust.mapper.UserMapper">

    <select id="findAll" resultType="cn.swust.pojo.User">
        select * from tb_user
    </select>
    <select id="findById" resultType="cn.swust.pojo.User">
        select * from tb_user where id = #{id}
    </select>
</mapper>